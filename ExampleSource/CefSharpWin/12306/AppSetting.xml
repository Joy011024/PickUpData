<?xml version="1.0" encoding="utf-8" ?>
<SystemSetting>
  <!--参数视图参数 mas，aes点击一次变化数值-->
  <OneClickValue>5</OneClickValue>
  <!--创建就诊信息之后自动关闭就诊人员维护窗体-->
  <CreatePatientAutoClose>true</CreatePatientAutoClose>
  <!--边框的色彩-->
  <BorderRGB>00|00|00</BorderRGB>
  <!--影像series控件显示的背景色-->
  <ImageEleBackgroundRGB>00|00|00</ImageEleBackgroundRGB>
  <!--画笔占用的像素-->
  <PenMargin>5</PenMargin>
  <!--默认情况下元素高度-->
  <NormalEleHeight>21</NormalEleHeight>
  <SeriesFontColor></SeriesFontColor>
  <!--车站数据来源URL-->
  <StationAPI>https://kyfw.12306.cn/otn/resources/js/framework/favorite_name.js</StationAPI>
  <!--正则解析车站-->
  <StationUrlRegexFormat>='(.*?)'</StationUrlRegexFormat>
  <ResourceDir>12306\</ResourceDir>
  <StationJsonFile>Station.txt</StationJsonFile>
  <!--一个车站包含多少个车站-->
  <StationFieldNum>6</StationFieldNum>
  <StationSplitRegex>(?@[\w|-]*)</StationSplitRegex> <!--(.*?)@--> <!--(.*?)@  第一个元素为空元素-->
  <!--余票查询接口-->
  <TicketQueryAPI>https://kyfw.12306.cn/otn/leftTicket/queryA?leftTicketDTO.train_date=2018-09-13&amp;leftTicketDTO.from_station=BJP&amp;leftTicketDTO.to_station=SHH&amp;purpose_codes=ADULT</TicketQueryAPI>
  <FakeIPHaiAPI>http://www.iphai.com/free/wg</FakeIPHaiAPI>
  <FakeIPAPIXiciaili>http://www.xicidaili.com/nn/1</FakeIPAPIXiciaili>
  <!--使用哪个ip代理来源-->
  <FakeIPAPISwitchInPool>FakeIPHaiAPI</FakeIPAPISwitchInPool>
  <SplitFlag>|</SplitFlag>
  <OpenDebug>false</OpenDebug>
  <!--ip地址列表分离正则表达式  <table ?? </tbody  < &lt; > &gt;-->
  <IPsProxyRegex>table-hover&quot;&gt;(.*?)&lt;/table</IPsProxyRegex>
  <!--IP代理详细信息分类-->
  <IPDataRegex></IPDataRegex>
  <IPEntityRegex></IPEntityRegex>
  <IPPoolMapData>IP=IP|端口号=Port|匿名度=Cryptonym|IP类型=IPHttpType|位置=IPAddress|响应速度=IPResponseSpleed|更新时间=IPPoolUpdateTimeDesc</IPPoolMapData>
<!--解析  <th>(.*?)</th>-->  
  <ColumnMapInHead desc="表格列解析">&lt;th&gt;(.*?)&lt;/th&gt;</ColumnMapInHead>
  <ValueMapInHead desc="数据解析">&lt;td&gt;(.*?)&lt;/td&gt;</ValueMapInHead>
</SystemSetting>
