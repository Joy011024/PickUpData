<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <SubwaySiteData>
      <add key="Insert" value="insert into  SubwaySiteData
(Id,InDBDayInt,CreateTime,BelongLine,TurnLine,SubwaySiteName,SubwaySiteTimeLong)
values
(
	'{Id}',{InDBDayInt},'{CreateTime}',{BelongLine},{TurnLine},'{SubwaySiteName}',{SubwaySiteTimeLong}
)" />
    </SubwaySiteData>
  <CitySubway>
      <add  key="Insert" value="insert into SubwayCollection
(Id,Name,Code,City) values('{Id}','{Name}',{Code},'{City}')"/>
  </CitySubway>
  </appSettings>
</configuration>